package Tubes;

import java.util.Scanner;

import java.util.Scanner;


public class Mesin {
	int N=2;
	Nasabah[] a=new Nasabah[N];
	
	void input() {
		Nasabah x;
		Scanner sc = new Scanner(System.in);
		
		for (int i=0; i<N ; i++) {
			x = new Nasabah();
			
			x.isiNasabah();;;
			a[i]=x;
		}
	}
	
	void tampilarray() {
		for (int i = 0; i < N ; i++) {
			System.out.println("array ke "+i+":");
			 a[i].tampil();
		}
	}

	public static void main(String[] args) {
		Mesin A=new Mesin();
		A.input();
		System.out.println();
		A.tampilarray();
	}

}
